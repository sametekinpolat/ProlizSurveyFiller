{
  "manifest_version": 3,
  "name": "Proliz Auto Survey Filler",
  "version": "1.0",
  "description": "Automatically fills surveys on University Student systems in Turkey.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png", 
      "48": "icon.png",
      "128": "icon.png"
    }
    
    
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}